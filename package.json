{
  "name": "microscope-angular",
  "version": "1.0.0",
  "description": "AngularJS (CommonJS/ browserify) project template for with browserify and grunt build system",
  "scripts": {
    "start": "grunt serve",
    "test": "grunt test"
  },
  "author": "Benjamin Heintz",
  "license": "MIT",
  "keywords": [
    "angularjs",
    "grunt",
    "build system",
    "browserify",
    "commonjs"
  ],
  "dependencies": {
    "angular": "^1.3.11",
    "angular-route": "^1.3.11",
    "bootstrap": "^3.3.2",
    "jquery": "^2.1.3"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "angular": "./node_modules/angular/angular.js"
  },
  "browserify-shim": {
    "angular": "angular"
  },
  "devDependencies": {
    "browserify-shim": "^3.8.2",
    "grunt": "^0.4.5",
    "grunt-browserify": "^3.3.0",
    "grunt-concurrent": "^1.0.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-connect": "^0.9.0",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-jshint": "^0.11.0",
    "grunt-contrib-uglify": "^0.7.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-docco": "^0.3.3",
    "jshint-stylish": "^1.0.0"
  }
}
